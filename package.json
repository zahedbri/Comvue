{
  "name": "garage-sale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "api": "nodemon server/index.js",
    "prod": "http-serve dist -p 5000 -g -c0 -S -C cert.pem -K key.pem",
    "start": "http-serve dist -p 5000 -g -c0",
    "start_bk": "node server.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "apollo-boost": "^0.1.16",
    "babel-plugin-transform-imports": "^1.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "cosmicjs": "^3.3.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "http-serve": "^1.0.1",
    "ios-inner-height": "^1.1.0",
    "nodemon": "^1.18.3",
    "register-service-worker": "^1.0.0",
    "serve": "^9.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue": "^2.5.16",
    "vue-cli-plugin-vuetify": "^0.1.6",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuetify": "^1.0.19",
    "vuex": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-unmodified-loop-condition": "off",
      "indent": [
        "error",
        4
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
